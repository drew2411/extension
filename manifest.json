{
  "manifest_version": 3,
  "name": "Content Identifier",
  "version": "1.0",
  "description": "Identifies if the current page is YouTube, Reddit, or other.",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "http://127.0.0.1:8000/"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}